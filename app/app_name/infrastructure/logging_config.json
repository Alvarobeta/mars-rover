{
   "version":1,
   "disable_existing_loggers": false,
   "formatters":{
      "simple":{
         "format":"'%(levelname)s|%(module)s|%(message)s'"
      },
      "verbose":{
         "format":"%(levelname)s|%(asctime)s|%(module)s|%(process)d|%(thread)d|%(message)s"
      }
   },
   "handlers":{
      "console":{
         "class":"logging.StreamHandler",
         "level":"DEBUG",
         "formatter":"simple",
         "stream":"ext://sys.stdout"
      }
   },
   "loggers":{
      "app":{
         "handlers":[
            "console"
         ],
         "level":"DEBUG",
         "propagate": false
      }
   },
   "root":{
      "level":"INFO",
      "handlers":[
         "console"
      ]
   }
}